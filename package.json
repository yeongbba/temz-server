{
  "name": "temz-server",
  "version": "1.0.0",
  "description": "temz-server",
  "main": "index",
  "repository": "git@github.com:yeongbba/temz-server.git",
  "author": "yeongbba <yeongmo.lee@logpresso.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"tsc -w\" \"nodemon dist/src/index\"",
    "start:prod": "node dist/src/index",
    "copyfile": "rm -rf dist/src/api && mkdir dist/src/api && cp -r src/api/ dist/src/api/",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "DOTENV_CONFIG_PATH=./.env.test jest --watchAll --verbose --globalTeardown=./src/tests/integration-tests/global-teardown.ts",
    "test:unit": "DOTENV_CONFIG_PATH=./.env.test jest --watch --verbose --testPathIgnorePatterns=/tests/integration-tests",
    "test:integration": "DOTENV_CONFIG_PATH=./.env.test jest --watchAll --verbose --testPathPattern=/tests/integration-tests --globalTeardown=./src/tests/integration-tests/global-teardown.ts",
    "test:watch": "DOTENV_CONFIG_PATH=./.env.test jest --watch",
    "test:watch:verbose": "DOTENV_CONFIG_PATH=./.env.test jest --watch --verbose",
    "test:coverage": "DOTENV_CONFIG_PATH=./.env.test jest --coverage",
    "test:unit:gh": "jest --testPathIgnorePatterns=/tests/integration-tests"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.1.4",
    "express": "5.0.0-beta.1",
    "express-openapi-validator": "^5.0.4",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.2.2",
    "morgan": "^1.10.0",
    "rate-limit-redis": "^3.0.2",
    "redis": "^4.6.7",
    "socket.io": "^4.6.2",
    "swagger-ui-express": "^4.6.3",
    "twilio": "^4.11.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-yml": "^1.7.0",
    "jest": "^29.5.0",
    "node-mocks-http": "^1.12.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "yarn-audit-fix": "^9.3.10"
  },
  "packageManager": "yarn@3.6.0"
}
