{
  "name": "temz-server",
  "version": "1.0.0",
  "description": "temz-server",
  "main": "index.ts",
  "repository": "git@github.com:yeongbba/temz-server.git",
  "author": "yeongbba <yeongmo.lee@logpresso.com>",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"tsc -w\" \"yarn run copyfile\" \"nodemon dist/src/index\"",
    "copyfile": "rm -rf dist/src/api && mkdir dist/src/api && cp src/api/openapi.yaml dist/src/api/",
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "test": "DOTENV_CONFIG_PATH=./.env.test jest",
    "test:watch": "DOTENV_CONFIG_PATH=./.env.test jest --watch",
    "test:watch:verbose": "DOTENV_CONFIG_PATH=./.env.test jest --watch --verbose",
    "test:verbose": "DOTENV_CONFIG_PATH=./.env.test jest --verbose",
    "test:coverage": "DOTENV_CONFIG_PATH=./.env.test jest --coverage"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "5.0.0-beta.1",
    "express-openapi-validator": "^5.0.2",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.0.1",
    "mongoose": "^6.9.2",
    "morgan": "^1.10.0",
    "socket.io": "^4.6.0",
    "swagger-ui-express": "^4.6.2",
    "twilio": "^4.7.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.4.3",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "node-mocks-http": "^1.12.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
